# Food Delivery Website Environment Variables
# Copy this file to .env.local and configure your specific values

# API Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_API_KEY=your_api_key_here

# Application Configuration
REACT_APP_APP_NAME=Food Delivery Website
REACT_APP_VERSION=1.0.0

# Third-party Services (for future implementation)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key_here
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_NOTIFICATIONS=false
REACT_APP_ENABLE_OFFLINE_MODE=false

# Debug Settings
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info 